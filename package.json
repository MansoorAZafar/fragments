{
  "name": "fragments",
  "private": true,
  "version": "0.11.0",
  "description": "Backend Microservices Project for DPS955 Cloud Computing For Programmers",
  "scripts": {
    "test": "jest -c jest.config.js --runInBand --",
    "test:watch": "jest -c jest.config.js --runInBand --watch --",
    "test:integration": "hurl --test --glob \"tests/integration/**/*.hurl\"",
    "test:lab10": "hurl --test \"tests/integration/lab-10-dynamodb.hurl\"",
    "test:lab9": "hurl --test \"tests/integration/lab-9-s3.hurl\"",
    "test:health-check": "hurl --test \"tests/integration/health-check.hurl\"",
    "coverage": "jest -c jest.config.js --runInBand --coverage",
    "lint": "eslint \"./src/**/*.js\" \"tests/**/*.js\"",
    "start": "node src/index.js",
    "dev": "cross-env LOG_LEVEL=debug nodemon ./src/index.js --watch src",
    "debug": "cross-env LOG_LEVEL=debug nodemon --inspect=0.0.0.0:9229 ./src/index.js --watch src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MansoorAZafar/fragments"
  },
  "keywords": [],
  "author": "Mansoor Zafar",
  "license": "UNLICENSED",
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@orangeopensource/hurl": "^6.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "3.4.2",
    "supertest": "^7.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.782.0",
    "@aws-sdk/client-s3": "^3.777.0",
    "@aws-sdk/lib-dynamodb": "^3.782.0",
    "aws-jwt-verify": "^5.0.0",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "fragments": "file:",
    "helmet": "^8.0.0",
    "http-auth": "^4.2.0",
    "http-auth-passport": "^1.0.7",
    "js-yaml": "^4.1.0",
    "markdown-it": "^14.1.0",
    "papaparse": "^5.5.2",
    "passport": "^0.7.0",
    "passport-http-bearer": "^1.0.1",
    "pino": "^9.6.0",
    "pino-http": "^10.3.0",
    "pino-pretty": "^13.0.0",
    "sharp": "^0.34.1",
    "stoppable": "^1.1.0",
    "striptags": "^3.2.0"
  }
}
